{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "launch",
            "type": "lua",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${workspaceRoot}/src/main.lua",
            "cwd": "${workspaceRoot}/bin",
            "path": "${workspaceRoot}/bin/?.lua",
            "cpath": "${workspaceRoot}/bin/?.dll;",
            "luaRuntime": "5.4 32bit",
            "arg": [
                "${workspaceRoot}/war3map.j"
            ],
            "consoleCoding": "utf8",
            "sourceCoding": "utf8"
        },
    ]
}